{
	"contentTypeUri": "https://amplience.com/composablecommerce/image.json",
	"settings": {
		"label": "Image",
		"visualizations": [
			{
				"label": "Visualization",
				"templatedUri": "https://{{vse.domain}}/v1/content/demo/content-item/{{content.sys.id}}?template=amp-template-visualization&locale={{locales}},en-*,*&vse={{vse.domain}}",
				"default": true
			},
			{
				"label": "JSON",
				"templatedUri": "https://presalesadisws.s3.eu-west-1.amazonaws.com/dynamic-content/dc-visualization-content-delivery/index.html?vse={{vse.domain}}&id={{content.sys.id}}&format=inlined&depth=all&v2=true&locale={{locales}}",
				"default": false
			}
		],
		"icons": [
			{
				"size": 256,
				"url": "https://bigcontent.io/cms/icons/potter/potter-image.png"
			}
		],
		"cards": [
			{
				"templatedUri": "https://{{vse.domain}}/v1/content/demo/content-item/{{content.sys.id}}?template=amp-template-cardsPreview&locale={{locales}},en-*,*"
			}
		]
	},
	"repositories": [
		"content"
	]
